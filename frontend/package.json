{
  "name": "hyperliquid-dashboard",
  "private": true,
  "version": "0.0.0",
  "description": "Hyperliquid Trading Bot Dashboard",
  "author": "Antigravity",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development electron .\" \"npm run dev\"",
    "electron:build": "npm run build && npx electron-builder"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.0",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.2.0"
  }
}